{
  "name": "SIPD CRAWLER",
  "version": "1.0.0",
  "description": "Browser extension untuk crawling data pada aplikasi SIPD",
  "manifest_version": 3,
  "short_name": "SIPD CRAWLER",
  "author": "Mochammad Fauzan Rizqulloh, S.Kom",
  "theme_color": "#000000",
  "background_color": "#ffffff",
  "action": {
    "default_popup": "index.html",
    "default_title": "SIPD CRAWLER"
  },
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "ContentScript.js"
      ]
    }
  ],
  "permissions": [
    "activeTab"
  ]
}